{
    "name": "celidone",
    "version": "1.0.0",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "dev": "electron . --dev",
        "dev:env": "cross-env NODE_ENV=development electron .",
        "build": "electron-builder",
        "test": "jest --detectOpenHandles",
        "test:watch": "jest --watchAll --detectOpenHandles",
        "test:coverage": "jest --coverage --detectOpenHandles",
        "test:unit": "jest --testPathPattern=unit --detectOpenHandles",
        "test:integration": "jest --testPathPattern=integration --detectOpenHandles",
        "lint": "eslint src/ --ext .js",
        "lint:fix": "eslint src/ --ext .js --fix",
        "format": "prettier --write src/",
        "format:check": "prettier --check src/",
        "clean": "rimraf dist/ node_modules/.cache/",
        "postinstall": "electron-builder install-app-deps",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/IWMVI/TCC.git"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/IWMVI/TCC/issues"
    },
    "homepage": "https://github.com/IWMVI/TCC#readme",
    "dependencies": {
        "axios": "^1.10.0",
        "dotenv": "^17.1.0",
        "electron": "^37.2.0",
        "pg": "^8.16.3",
        "pg-hstore": "^2.3.4",
        "react-dom": "^19.1.0",
        "sequelize": "^6.37.7"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.6.3",
        "cross-env": "^7.0.3",
        "electron-reload": "^2.0.0-alpha.1",
        "eslint": "^9.30.1",
        "jest": "^30.0.4",
        "jest-environment-jsdom": "^30.0.4",
        "sqlite3": "^5.1.7"
    },
    "jest": {
        "testEnvironment": "jsdom",
        "setupFilesAfterEnv": [
            "<rootDir>/jest.setup.js"
        ]
    },
    "description": ""
}
